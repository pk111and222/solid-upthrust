{
  "name": "upthrust-ui",
  "version": "0.0.1",
  "description": "A simple solid ui lib built with unocss",
  "keywords": [
    "solid",
    "unocss",
    "vite",
    "component",
    "ui",
    "solid admin",
    "solid component"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/upthrust.umd.cjs",
      "import": "./dist/upthrust.js"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "main": "./dist/upthrust.cjs",
  "unpkg": "./dist/upthrust.umd.cjs",
  "module": "./dist/upthrust.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "watch": "vite build --mode watch",
    "build": "vite build",
    "test": "vite test"
  },
  "license": "MIT",
  "peerDependencies": {
    "solid-js": "^1.8.17"
  },
  "devDependencies": {
    "class-variance-authority": "^0.7.0",
    "tailwind-merge": "^2.3.0",
    "upthrust-unocss-preset": "workspace:*",
    "upthrust-competence": "workspace:*",
    "@unocss/preset-icons": "^0.61.0",
    "@iconify-json/mdi": "^1.1.66",
    "@unocss/preset-wind": "^0.61.0",
    "@unocss/transformer-compile-class": "^0.61.0",
    "clsx": "^2.1.1"
  }
}
